{
    "local-irc": {
        "type": "port",
        "port": 6667,
        "target": "localhost",
        "display": true,
        "display-name": "Local IRC server"
    },
    "local-ssh": {
        "type": "port",
        "port": 22,
        "target": "localhost",
        "display": false,
        "display-name": "Local SSH server"
    },
    "local-ldap": {
        "type": "port",
        "port": 636,
        "target": "localhost",
        "display": true,
        "display-name": "Local LDAP server"
    },
    "local-services": {
        "type": "psuedo",
        "logic": "all",
        "deps": ["local-irc", "local-ssh", "local-ldap"],
        "display": true,
        "display-name": "Local services"
    }
}
